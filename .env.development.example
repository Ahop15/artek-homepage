# Google Maps Configuration
# ========================================
VITE_GOOGLE_MAPS_API_KEY=
VITE_GOOGLE_MAP_ID=

# ========================================
# ARTEK AI Configuration
# ========================================
# Mode: "prod" | "dev" | "direct"
# - prod: Use production proxy (VITE_ARTEK_AI_URL)
# - dev: Use local wrangler proxy (VITE_ARTEK_AI_URL_DEV)
# - direct: Use direct AI-Worker
VITE_ARTEK_AI_MODE=dev

# Proxy URLs
VITE_ARTEK_AI_URL=
VITE_ARTEK_AI_URL_DEV=

# Turnstile
VITE_ARTEK_AI_TURNSTILE_DEV_SITE_KEY=1x00000000000000000000AA
VITE_ARTEK_AI_TURNSTILE_SITE_KEY=

# ========================================
# Contact Form Configuration
# ========================================
# Mode: "prod" | "dev"
# - prod: Use production worker (VITE_MAIL_WORKER_ENDPOINT)
# - dev: Use development worker (VITE_MAIL_WORKER_DEV_ENDPOINT)
VITE_MAIL_WORKER_MODE=dev

# Worker URLs
VITE_MAIL_WORKER_ENDPOINT=
VITE_MAIL_WORKER_DEV_ENDPOINT=

# Turnstile Site Keys
# Test site key - always passes (for development with VITE_MAIL_WORKER_MODE=dev)
VITE_TURNSTILE_DEV_SITE_KEY=1x00000000000000000000AA
# Production site key (for VITE_MAIL_WORKER_MODE=prod)
VITE_TURNSTILE_SITE_KEY=